    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const methodBtns = document.querySelectorAll('.method-btn');
            const inputAreas = document.querySelectorAll('.input-area');
            const uploadArea = document.getElementById('upload-area');
            const fileInput = document.getElementById('file-input');
            const analyzeButton = document.getElementById('analyze-button');
            const loadingIndicator = document.getElementById('loading-indicator');
            const resultsSection = document.getElementById('results-section');
            const materialNameSpan = document.getElementById('material-name');
            const textField = document.getElementById('text-field');

            methodBtns.forEach(btn => {
                btn.addEventListener('click', () => {
                    methodBtns.forEach(b => b.classList.remove('active'));
                    btn.classList.add('active');

                    inputAreas.forEach(area => area.classList.remove('active'));
                    document.getElementById(`${btn.dataset.method}-input`).classList.add('active');
                });
            });

            uploadArea.addEventListener('click', () => {
                fileInput.click();
            });

            uploadArea.addEventListener('dragover', (e) => {
                e.preventDefault();
                uploadArea.style.borderColor = `var(--primary-color)`;
            });

            uploadArea.addEventListener('dragleave', (e) => {
                e.preventDefault();
                uploadArea.style.borderColor = `var(--accent-color)`;
            });

            uploadArea.addEventListener('drop', (e) => {
                e.preventDefault();
                uploadArea.style.borderColor = `var(--accent-color)`;
                const files = e.dataTransfer.files;
                if (files.length > 0) {
                    handleFiles(files);
                }
            });

            fileInput.addEventListener('change', (e) => {
                handleFiles(e.target.files);
            });

            function handleFiles(files) {
                if (files.length > 0) {
                    const file = files[0];
                    // In a real application, you would upload the file to a server
                    console.log('File selected:', file.name);
                    // Optionally, display a preview or file name
                    uploadArea.querySelector('h3').textContent = `File: ${file.name}`;
                    uploadArea.querySelector('p').textContent = 'Ready for analysis';
                }
            }

            analyzeButton.addEventListener('click', () => {
                loadingIndicator.classList.add('active');
                resultsSection.classList.remove('active');
                // Simulate API call
                setTimeout(() => {
                    loadingIndicator.classList.remove('active');
                    resultsSection.classList.add('active');

                    let material = '';
                    if (document.getElementById('image-input').classList.contains('active')) {
                        material = fileInput.files.length > 0 ? fileInput.files[0].name.split('.')[0] : 'unknown image';
                    } else {
                        material = textField.value || 'unspecified material';
                    }
                    materialNameSpan.textContent = material;

                }, 3000); // 3 seconds delay
            });
        });
    </script>
